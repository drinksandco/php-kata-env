{
    "name": "uvinum/php-kata-env",
    "type": "project",
    "description": "Uvinum skeleton repository to start with our coding katas.",
    "homepage": "https://github.com/uvinum/php-kata-env",
    "keywords": [
        "kata",
        "uvinum",
        "testing"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Segovia",
            "email": "velozmarkdrea@gmail.com",
            "homepage": "http://www.marcossegovia.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "mikey179/vfsStream": "^1.6"
    },
    "require-dev": {
        "symfony/var-dumper": "~3.0",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9.5",
        "behat/behat": "~3.2",
        "scrutinizer/ocular": "~1.3",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Kata\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KataTest\\": "tests"
        }
    },
    "scripts": {
        "test-unit": "phpunit",
        "test-integration": "php vendor/bin/behat --suite example",
        "format": "phpcbf --standard=psr2 src/"
    },
    "minimum-stability": "stable"
}
